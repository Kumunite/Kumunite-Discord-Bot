{
    "name": "kumunite-discord-bot",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "forever npm run dev",
        "prepare": "npm run build && npm run test",
        "test": "mocha -r ts-node/register src/tests/**/*.test.ts",
        "coverage": "nyc mocha -r ts-node/register test/**/*.test.ts",
        "build": "tsc -p ./tsconfig.json",
        "dev": "npm run build && ts-node -r dotenv/config ./dist/index.ts",
        "format": "prettier --write --config ./.prettierrc ./src",
        "lint": "eslint . --ext .ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Kumunite/Kumunite-Discord-Bot.git"
    },
    "author": "okanaslan",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Kumunite/Kumunite-Discord-Bot/issues"
    },
    "homepage": "https://github.com/Kumunite/Kumunite-Discord-Bot#readme",
    "dependencies": {
        "@discordjs/opus": "^0.3.2",
        "discord.js": "^12.3.1",
        "dotenv": "^8.2.0",
        "ffmpeg-static": "^4.2.7",
        "forever": "^3.0.2",
        "ytdl-core": "^3.3.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.12",
        "@types/dotenv": "^8.2.0",
        "@types/mocha": "^8.0.3",
        "@types/node": "^14.11.2",
        "@types/prettier": "^2.1.1",
        "@typescript-eslint/eslint-plugin": "^4.3.0",
        "@typescript-eslint/parser": "^4.3.0",
        "chai": "^4.2.0",
        "eslint": "^7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "mocha": "^8.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.1.2",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.3"
    }
}
